{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "description": "Neat hovercards for GitHub.",
   "externally_connectable": {
      "matches": [ "*://justineo.github.io/*" ]
   },
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "GitHub Hovercard",
   "optional_permissions": [ "\u003Call_urls>" ],
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "webNavigation", "tabs", "storage", "https://github.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.8.0"
}
